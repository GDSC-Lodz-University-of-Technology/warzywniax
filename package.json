{
  "name": "warzywniax",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "git config core.hooksPath ./git-hooks && npm run git:stop-tracking-db-seed",
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:ts": "eslint . --cache --ext .ts --ext .tsx",
    "lint:ts:fix": "eslint . --cache --ext .ts --ext .tsx --fix",
    "lint:ts:watch": "esw . --watch --cache --ext .ts --ext .tsx ",
    "prettyprint": "prettier --check .",
    "prettyprint:fix": "prettier --write .",
    "firebase:run:emulator": "cd firebase && firebase emulators:start --project=warzywniax --import=./seed --export-on-exit",
    "firebase:deploy": "cd firebase && firebase deploy --only storage,firestore:rules,firestore:indexes",
    "firebase:deploy:storage-rules": "cd firebase && firebase deploy --only storage",
    "firebase:deploy:firestore-rules": "cd firebase && firebase deploy --only firestore:rules",
    "firebase:deploy:firesotre-indexes": "cd firebase && firebase deploy --only firestore:indexes",
    "git:stop-tracking-db-seed": "git update-index --assume-unchanged firebase/seed/auth_export/accounts.json firebase/seed/auth_export/config.json firebase/seed/firebase-export-metadata.json firebase/seed/firestore_export/all_namespaces/all_kinds/all_namespaces_all_kinds.export_metadata firebase/seed/firestore_export/all_namespaces/all_kinds/output-0 firebase/seed/firestore_export/firestore_export.overall_export_metadata firebase/seed/storage_export/buckets.json",
    "test:integration": "jest /test/integration --passWithNoTests --setupFilesAfterEnv ./test/integration/integration-setup.ts",
    "test:integration:watch": "jest /test/integration --passWithNoTests --setupFilesAfterEnv ./test/integration/integration-setup.ts --watch"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@googlemaps/js-api-loader": "^1.15.1",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.10",
    "@tanstack/react-query": "^4.24.10",
    "firebase": "^9.17.1",
    "i18next": "^22.4.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.1.5",
    "react-router-dom": "^6.8.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@firebase/rules-unit-testing": "^2.0.7",
    "@swc/core": "^1.3.42",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-watch": "^8.0.0",
    "firebase-tools": "^11.24.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-tsconfig-paths": "^4.0.7"
  }
}
